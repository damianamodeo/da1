{
  "name": "app-orderly",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/orderly/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/orderly"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app-orderly:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "app-orderly:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "app-orderly:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "app-orderly:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "app-orderly:build:development"
        },
        "production": {
          "buildTarget": "app-orderly:build:production"
        }
      }
    }
  },
  "tags": []
}
